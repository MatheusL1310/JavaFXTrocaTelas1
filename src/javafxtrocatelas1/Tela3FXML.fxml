          <CategoryAxis label="Materias" side="BOTTOM" fx:id="x">
               <tickLabelFont>
                  <Font size="14.0" />
               </tickLabelFont></CategoryAxis>
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="y" label="Minha nota" side="LEFT">
               <tickLabelFont>
                  <Font size="14.0" />
               </tickLabelFont></NumberAxis>
        </yAxis>
      </BarChart>
      <Button fx:id="voltaTela1" layoutX="75.0" layoutY="651.0" mnemonicParsing="false" onAction="#onActionChoiceBox" prefHeight="48.0" prefWidth="180.0" text="Voltar">
         <font>
            <Font size="24.0" />
         </font></Button>
      <ChoiceBox id="disciplina" fx:id="campoDisciplina" layoutX="431.0" layoutY="658.0" onKeyPressed="#onActionChoiceBox" prefHeight="36.0" prefWidth="425.0" />
